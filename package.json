  "main": "background.js",
  "devDependencies": {
    "@types/lodash": "^4.14.117",
    "@types/tape": "^4.2.32",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.4",
    "moment": "^2.22.2",
    "node-sass": "^4.9.4",
    "nyc": "^11.9.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.1",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-loader": "^15.4.2",
    "vue-property-decorator": "^6.1.0",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
